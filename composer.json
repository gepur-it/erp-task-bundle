{
    "name": "gepur-it/erp-task-bundle",
    "description": "Erp task bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "version": "6.0.0",
    "keywords": [
      "gepur",
      "symfony",
      "bundle",
      "calltask"
    ],
    "homepage": "https://github.com/gepur-it/erp-task-bundle",
    "require": {
