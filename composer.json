        "symfony/validator": "^4.3|^5.0",
        "symfony/doctrine-bridge": "^4.3|^5.0",
        "doctrine/orm": ">=2.6",
        "symfony/dependency-injection": "^4.3|^5.0",
        "symfony/config": "^4.3|^5.0",
        "symfony/event-dispatcher": "^4.3|^5.0",
        "symfony/http-kernel": "^4.3|^5.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
    "psr-4": {
      "GepurIt\\ErpTaskBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GepurIt\\ErpTaskBundle\\Tests\\": "Tests"
    }
  },
    "authors": [
      {
        "name": "Andrii Yakovlev",
