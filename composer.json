{
    "name": "gepur-it/erp-task-bundle",
    "description": "Erp task bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "version": "1.1.0",
    "keywords": [
      "gepur",
      "symfony",
      "bundle",
      "calltask"
    ],
    "homepage": "https://github.com/gepur-it/erp-task-bundle",
    "require": {
        "php": "^7.2",
        "symfony/validator": "^4.2",
        "symfony/doctrine-bridge": "^4.2",
        "doctrine/orm": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
  "autoload": {
    "psr-4": {
      "GepurIt\\ErpTaskBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GepurIt\\ErpTaskBundle\\Tests\\": "Tests"
    }
  },
    "authors": [
      {
        "name": "Andrii Yakovlev",
        "email": "yawa20@gmail.com"
      },
      {
        "name": "Victor Pavlov",
        "email": "zogxray@gmail.com"
      }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
